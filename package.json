{
  "name": "unpinned",
  "version": "0.0.0-semantic-release",
  "description": "Checks your package.json for unpinned dependencies",
  "main": "index.js",
  "bin": {
    "unpinned": "cli.js"
  },
  "scripts": {
    "precommit": "npm test",
    "commit": "git-cz",
    "test": "jest --coverage",
    "posttest": "standard"
  },
  "keywords": [
    "npm",
    "modules",
    "versions"
  ],
  "author": "herrmannplatz <rene@herrmannplatz.de>",
  "license": "MIT",
  "dependencies": {
    "minimist": "1.2.0"
  },
  "devDependencies": {
    "commitizen": "3.0.4",
    "cz-conventional-changelog": "2.1.0",
    "husky": "1.1.1",
    "jest": "23.4.2",
    "semantic-release": "15.10.3",
    "standard": "12.0.0"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/herrmannplatz/unpinned.git"
  },
  "config": {
    "commitizen": {
      "path": "cz-conventional-changelog"
    }
  },
  "jest": {
    "verbose": true,
    "testURL": "http://localhost/"
  }
}
