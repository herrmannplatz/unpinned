{
  "name": "unpinned",
  "version": "0.0.0-semantic-release",
  "description": "Checks your package.json for unpinned dependencies",
  "main": "index.js",
  "bin": {
    "unpinned": "cli.js"
  },
  "scripts": {
    "test": "jest --coverage",
    "posttest": "standard",
    "prepare": "husky install"
  },
  "keywords": [
    "npm",
    "modules",
    "versions"
  ],
  "author": "herrmannplatz <rene@herrmannplatz.de>",
  "license": "MIT",
  "dependencies": {
    "minimist": "1.2.5"
  },
  "devDependencies": {
    "cz-conventional-changelog": "3.3.0",
    "husky": "7.0.2",
    "jest": "27.2.0",
    "semantic-release": "18.0.0",
    "standard": "16.0.3"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/herrmannplatz/unpinned.git"
  },
  "config": {
    "commitizen": {
      "path": "cz-conventional-changelog"
    }
  },
  "jest": {
    "verbose": true,
    "testURL": "http://localhost/"
  }
}
